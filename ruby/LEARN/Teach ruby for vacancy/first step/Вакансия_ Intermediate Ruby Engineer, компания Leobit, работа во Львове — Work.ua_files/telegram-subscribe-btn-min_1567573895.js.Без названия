var telegramSubscribeBtn={init:function(){telegramSubscribeBtn.hideErrorAlert(),$(document).on("click","#telegramSubscribeBtnBlock",function(e){e.preventDefault();var r=$(this).find("#telegramSubscribeBtn");telegramSubscribeBtn.click(r)})},click:function(e){if(1===e.data("employer"))return void telegramSubscribeBtn.showErrorAlert(lang.jobseekerSearchAgent.employerAlert);var r=e.attr("href");if("javascript:void(0);"!==r&&"#"!==r){var t=r.split("?")[1];""!==t&&(t="?"+t),$.ajax({url:Work.localeService.getURLPrefix()+"/jobseeker/search-agents/telegram/ajax/create/"+t,success:function(e){e&&e.created?telegramSubscribeBtn.showSuccessAlert(e.message):e&&e.error?telegramSubscribeBtn.showErrorAlert(e.message):document.location.href=r}})}},showSuccessAlert:function(e){telegramSubscribeBtn._hideAnotherErrorAlerts(),this.hideErrorAlert(),$("#telegramSubscribeBtnBlock").hide(),$("#telegramSubscribeBtnSuccessMessage").find("p").text(e),$("#telegramSubscribeBtnSuccessMessage").show()},showErrorAlert:function(e){telegramSubscribeBtn._hideAnotherErrorAlerts(),$("#telegramSubscribeBtnErrorMessage").find("p").html(e),$("#telegramSubscribeBtnErrorMessage").show()},hideErrorAlert:function(){$("#telegramSubscribeBtnErrorMessage").hide()},_hideAnotherErrorAlerts:function(){$("#jsaF0").find(".form-group").removeClass("has-danger"),$("#jsaF0").find(".text-danger").text("")}};telegramSubscribeBtn.init();