$===jQuery&&$(document).ready(function(){$('[data-spy="affix"]').each(function(){var o,t=$(this),i=t.offset().top,f=t.data("offset-bottom")||null;t.hasClass("sidebar")?i-=20:t.hasClass("menu-action-top")&&(t.hasClass("menu-action-bottom-xs")?t.wrap('<div id="fix-block" class="fix-block-bottom-xs"></div>'):t.wrap('<div id="fix-block"></div>'),o=$("#fix-block"),t.on("affix.bs.affix",function(){o.css({height:t.height()})}),t.on("affix-top.bs.affix",function(){o.css({height:"auto"})})),t.affix({offset:{top:function(){return t.hasClass("menu-action-top")?o.offset().top:i},bottom:f}}),t.affix("checkPosition")})});